stop();
var bgId:String;
bgBtn1.addEventListener(MouseEvent. CLICK, selectbg1);
function selectbg1(event:MouseEvent):void
{


	gotoAndStop(15);
	bgId = "1";
	gotoAndStop(15);
	trace(bgId);


}
